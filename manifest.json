{
  "manifest_version": 3,
  "name": "Omnisearch",
  "version": "1.2.2",
  "description": "Search multiple platforms at once with a single query.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon-16.png",
      "32": "icon-32.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    }
  },
  "permissions": ["tabs", "storage"],
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "options_page": "settings.html"
} 